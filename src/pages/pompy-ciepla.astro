---
import co1 from "@/assets/photovoltaics/fotowoltaika2.webp";
import co2 from "@/assets/photovoltaics/fotowoltaika3.webp";
import AnimatedSectio from "@/components/AnimatedSectio.astro";
import Landing from "@/sections/HeatPumps/Landing.astro";
import SeoV1 from "@/sections/HeatPumps/SeoV1.astro";
import SeoV2 from "@/sections/HeatPumps/SeoV2.astro";
import Layout from "../layouts/Layout.astro";
const items = [
  { text: "Dobór urządzeń", img: co1 },
  { text: "Sprzedaż klimatyzacji", img: co2 },
  { text: "Montaż klimatyzacji", img: co1 },
  { text: "Konserwacja klimatyzacji", img: co2 },
];
