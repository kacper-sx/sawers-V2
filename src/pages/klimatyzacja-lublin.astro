---
import co2 from '@/assets/air-conditioning/klimatyzacja2.webp'
import co1 from '@/assets/air-conditioning/klimatyzacja3.webp'
import AnimatedSectio from '@/components/AnimatedSectio.astro'
import CTA from '@/components/CTA.astro'
import LandingLublin from '@/sections/AirCondition/Landing-Lublin.astro'
import SeoV1 from '@/sections/AirCondition/SeoV1.astro'
import SeoV2 from '@/sections/AirCondition/SeoV2.astro'
import LayoutLublin from '../layouts/Layout.astro'
import SeoV1Lublin from '../sections/AirCondition/SeoV1Lublin.astro'

const items = [
  { text: 'Dobór urządzeń', img: co1 },
  { text: 'Sprzedaż klimatyzacji', img: co2 },
  { text: 'Montaż klimatyzacji', img: co1 },
  { text: 'Serwis klimatyzacji', img: co2 },
]

